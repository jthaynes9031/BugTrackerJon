                                <div class="widget-text-box">
                                    <h4>Admin</h4>
                                    <p>Manage Roles and Assign Users to Projects.</p>
                                    <div class="text-right like-love-list">
                                        <button type="submit" class="btn btn-xs btn-primary">Login</button>
                                    </div>
                                </div>
                            </div>
                        }
                    </div>
                </div>
